
<% products.map((product) =>{ %>
<div class="card">
  <a href="/product/<%=product._id%>"><img class="image-body" src="<%=product.image%>" /></a>
  <div class="card-body">
    <a href="/product/<%=product._id%>"><h2><%=product.name%></h2></a>
    <%-include('./Rating.ejs',{rating:product.rating,numReviews:product.numReviews});%>
      <div class="price">ï¿¥<%=product.price.toLocaleString()%></div>
  </div>
</div>
<% }) %>
